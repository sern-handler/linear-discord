{
    "compilerOptions": {
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"target": "ESNext",
		"module": "ESNext",
		"outDir": "dist",
		"rootDir": "src",
		"strict": true,
		"esModuleInterop": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
    }
}
